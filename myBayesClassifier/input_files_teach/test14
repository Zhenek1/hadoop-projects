function jsneworder(getget) {
	callServer(null, '/get_base.php?consis='+Math.random()+'&'+getget, null, null, showHTML, 'indexbaseajax');

		document.getElementById('t01').className = '';
		document.getElementById('t02').className = '';
		document.getElementById('t03').className = '';
		document.getElementById('t04').className = '';
		document.getElementById('t05').className = '';

		if (getget.indexOf('t01') !=-1) {
			document.getElementById('t01').className = 'active';
		}
		if (getget.indexOf('t02') !=-1) {
			document.getElementById('t02').className = 'active';
		}
		if (getget.indexOf('t03') !=-1) {
			document.getElementById('t03').className = 'active';
		}
		if (getget.indexOf('t04') !=-1) {
			document.getElementById('t04').className = 'active';
		}
		if (getget.indexOf('t05') !=-1) {
			document.getElementById('t05').className = 'active';
		}
}

