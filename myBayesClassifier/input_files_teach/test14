	function ShowFancyBoxIframe(formurl,padding,width,height)
	{
		$.fancybox({
				'autoScale'          : false,
				'autoDimensions'     : false,
				'hideOnOverlayClick' : false,
				'href'               : formurl,
				'padding'            : padding,
				'showNavArrows'      : false,
				'type'               : 'iframe',
				'titleShow'          : false,
				'transitionIn'       : 'none',
				'transitionOut'      : 'none',
				'width'              : width,
				'height'             : height
		});
	}

