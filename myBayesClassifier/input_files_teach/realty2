<script type="text/javascript">
	var selected_home_region;

	if(document.referrer.indexOf('irr.ru') == -1){
		if ((selected_home_region = $.cookie(RegionChooser.HOME_REGION_COOKIE_NAME)) && document.location.hostname == 'irr.ru'
				&& (window.location.pathname == '' || window.location.pathname == '/') && window.location.pathname != selected_home_region){

			if ((selected_home_region.indexOf('http://') == 0)) {
				if ((window.location.href.replace(window.location.search,'') != selected_home_region)) {
					window.location.href = selected_home_region+window.location.search;
				}
			} else {
				window.location.pathname = selected_home_region+window.location.search;
			}
		}
	}
</script>




