	<!-- pageBlock: /applications/site/ssi/hot_offers_list/hot_offers_carousel.php -->
<div class="hot_offers_block clear">
	<script>
		$(document).ready(function(){
			var size_of_items = 18;
			var controls = true;
			var hots = $('.hotoffer_slider .ad_hot_offer_carousel');
			var height_ul_block = $('.hotoffer_slider.jcarousel').height();
			if ($('.hotoffer_slider .add_type3').size() <= 3) controls = false;

			$('.jcarousel').jcarousel({
				scroll: 1,
				itemVisibleInCallback: {
					onAfterAnimation: function(carousel, item, idx, state) {
						if (state != 'init') {
							count_hot(item);
						}

						if(idx >= (size_of_items - 1)){
							if (addAjaxItems(carousel, size_of_items, height_ul_block)){
								size_of_items = size_of_items * 2;
							}
						}
					}
				},
				initCallback: function() {
					count_hot(hots[0]);
					count_hot(hots[1]);
					count_hot(hots[2]);
					if (!controls) {
						$('.jcarousel-prev, .jcarousel-next').hide();
					}
				}
			});

			var Device = {};
			Device.isiPhone = navigator.userAgent.toLowerCase().indexOf("iphone");
			Device.isiPad = navigator.userAgent.toLowerCase().indexOf("ipad");
			Device.isAndroid = navigator.userAgent.toLowerCase().indexOf("android");

			if (Device.isiPhone > -1 || Device.isiPad > -1 || Device.isAndroid > -1) {
				$(".ad_hot_offer_carousel").removeClass("hovered");
				$(".ad_hot_offer_carousel:not(.hovered)").click(function(){
					if($(this).find(".add_info").css('display') == 'none'){
						$(".ad_hot_offer_carousel").find(".add_info").fadeOut();
						$(this).find(".add_info").fadeIn();
					}
				});
				$(".hotoffer_close").click(function(){
					$(this).parent().fadeOut();
				})
			}

		});
		function count_hot(el) {
			var place = $(el);
			if (place.hasClass('counted')) return false;
			place.addClass('counted');
			var counter_src = place.data('counter-src');
			if (counter_src && place.is(":visible")) {
				eval(Base64.decode(counter_src));
			}
			if (place.is(":visible")) {
				var zero_pixel = place.find('input.ho_zeropixel');
				if (zero_pixel.length == 1) {
					zero_pixel.before('<img width="1" height="1" border="0" src="' + zero_pixel.val() + '">');
				}
			}
		}

		function addAjaxItems(carousel, offset, height_ul_block) {
			$.ajax({
				type: 'POST',
				data:{
					region_id: 3 ,
					category_url : 'classified/real-estate/apartments-sale/',
					offset: offset
				},
				cache: false,
				url: '/ajax/2013/ajax_hot_offer_list.php',
				success: function (data) {
					$('.hotoffer_slider.jcarousel ul li:last').after(data);
					//console.log(data);
					var begin = carousel.last;
					var li = $('.hotoffer_slider.jcarousel ul li:gt('+ (begin)+')');
					li.height(height_ul_block);
					li.each(function(i, element){
						carousel.add(begin + i + 2, element);
					});
					carousel.size($('.hotoffer_slider.jcarousel ul li').size());

				}
			});
			return true;
		};
	</script>

