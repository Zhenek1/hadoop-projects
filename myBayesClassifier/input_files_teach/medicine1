                        var curdoc=1;
                        var alldoc=1;
                        
                        function PrevDoc(){
                            $('.doctor_item').html('<table width="100%" height="335px" ><tr><td style="vertical-align:middle;height: 100%;"><center><img src="/images/loading_line.gif"></center></td><tr></table>');
                            $.ajax({type:"POST",url:"/ajax/bitrixAction.php",data:{ACTION:"RANDOM_DOCTORS"},dataType:'html',success:function (html){$('.doctor_item').replaceWith(html);}});
                            // if(curdoc<=1) return;
                            // curdoc--;
                            
                            // ShowDoc();
                        }
                        
                        function NextDoc(){
                            $('.doctor_item').html('<table width="100%" height="335px" ><tr><td style="vertical-align:middle;height: 100%;"><center><img src="/images/loading_line.gif"></center></td><tr></table>');
                            $.ajax({type:"POST",url:"/ajax/bitrixAction.php",data:{ACTION:"RANDOM_DOCTORS"},dataType:'html',success:function (html){$('.doctor_item').replaceWith(html);}});
                            // if(curdoc>=alldoc) return;
                            // curdoc++;
                            // if(curdoc>=alldoc) $('#button-nav-next').addClass('disable-button-next'); 
                            // ShowDoc();
                        }
                        
                        function ShowDoc(){
                            if(curdoc<=1) {
                                $('#button-nav-prev').removeClass('button-nav-prev');
                                $('#button-nav-prev').addClass('disable-button-prev');
                            }    
                            else{
                                $('#button-nav-prev').removeClass('disable-button-prev');
                                $('#button-nav-prev').addClass('button-nav-prev');
                            
                            }
                            
                            if(curdoc>=alldoc) {
                                $('#button-nav-next').removeClass('button-nav-next');
                                $('#button-nav-next').addClass('disable-button-next');
                            }    
                            else{
                                $('#button-nav-next').removeClass('disable-button-next');
                                $('#button-nav-next').addClass('button-nav-next');
                            
                            }
                            $('.doctor_item').each(function(){$(this).hide();});
                            $('#doctor_'+curdoc).show();
                        }
                        
                        //ShowDoc();
                    </script>
