			function CloseModalWindow(){
				$('.bx-core-dialog-overlay').hide();
				$('.popup_doctors_email_booking').hide();
			}
			function SetIframeHeight(){
				iframe=$('#email_book_iframe')[0];
				height=$(iframe.contentWindow.document.getElementById('form_email')).height()+30;
				$(iframe).height(height);
				$('.popup_doctors_email_booking').height(height+30);
			}
			
			
