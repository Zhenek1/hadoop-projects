<script type="text/javascript">
			
			function BackCall(){
				phone=$('#phone').val();
				
				if(phone.length!=17){
					alert('Номер телефона введен неправильно.');
					return;
				}
				
				console.log(phone.length);
				$('#request_form').html('<br/><center><img id="bxid_458224" src="/images/loading_line.gif"  /></center>');
				if(phone.trim()=='') return;
				$.ajax({
					url:'/ajax/bitrixAction.php',
					data:"ACTION=BACK_CALL&PHONE="+phone+"&sessid=",
					type:"POST",
					success:function(html){console.log(html);$('#request_form').html('<br/><h4>В ближайшее время с вами свяжется администратор!</h4>');}
				});
			}
		</script>
