        <script src="/static/scripts/body.js"></script>
        <script>
            $(function() {
            	function setHomeCookie(name, value) {
                	var today = new Date(),
						expire = new Date();

					expire.setTime(today.getTime() + 3600000*24*365);
 					document.cookie = name+'='+value+ ';expires='+expire.toUTCString();
                }


                $('form[name=home_search_form]').on('submit', function(e) {
                    e.preventDefault();
                    var deal_type_str = $(this).find('select[name=deal_type]').val(),
                    	deal_type = 1,
                        offer_type = $(this).find('select[name=offer_type]').val(),
                        obl_id = $(this).find('select[name=obl_id]').val(),
                        query_offer_type = 'flats=yes';

                    if (deal_type_str === 'sale') deal_type = 2;
                    else deal_type = 1;

                    if (offer_type === 'suburban') query_offer_type = 'suburbian=yes';
                    else if (offer_type === 'commercial') query_offer_type = 'offices=yes';

                    setHomeCookie('default_region', obl_id);
					setHomeCookie('default_deal_type', deal_type_str);
					setHomeCookie('default_offer_type', offer_type);

                    document.location.href = '/nd/search/?'+'deal_type='+deal_type_str+'&'+query_offer_type+'&'+'obl_id='+obl_id;
                });
            });
        </script>
        <script type="text/javascript">
         $('body').toggleClass('opera_12', Detectizr.browser.name === 'opera' && parseInt(Detectizr.browser.major) < 13);
        </script>

        
            <script type="text/javascript">
                $(function() {
                    var subdomain = 'None',
                        obl_id = '1',
                        match = {
                            'spb': 'Санкт-Петербург',
                            'nn': 'Нижний Новгород',
                            'samara': 'Самара'
                        };

                    if (subdomain && typeof match[subdomain] !== 'undefined') {
                        _gaq.push(['_trackEvent', 'Regions', 'FirstPage', match[subdomain]]);
                    } else if (parseInt(obl_id) == 1) {
                        _gaq.push(['_trackEvent', 'Regions', 'FirstPage', 'Москва']);
                    }
                });
            </script>
        

        <input type="hidden" name="geo_obl_id" value="1">
        <input type="hidden" name="obl_id" value="1">

        <input type="hidden" name="GEOIP_COUNTRY_CODE" value="">
        <input type="hidden" name="GEOIP_REGION" value="48">
    </body>
</html>

